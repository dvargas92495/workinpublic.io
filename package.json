    "eslint": "^8.0.1",
    "fuegojs": "^0.16.4",
    "prettier": "^2.4.1",
    "ts-node": "^10.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.5.2"
